<h1>Networks</h1>

<% if @networks.public_methods.include?(:page_totals) %>
  <!-- only do the page links if the collection supports it -->
  <%= render :partial => "general/paginate_alphabetical", :locals => { :pages => @networks.pages, :page_totals=>@networks.page_totals,:current_page=>@networks.page } -%>
<% end %>

<div class="list_items_container">
  <%= render :partial=>"assets/resource_list_item", :collection=>@networks %>
</div>